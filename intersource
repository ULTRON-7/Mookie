def GetLSProData(page_data,Cookie_Jar,m):
 import re
 source = re.search(r"flashplayer.+'\/(.*?)'.*\s.*'(http.*?)'", page_data)
 return source.group(2) + '|Referer=http://urhd.tv/' + source.group(1)
]]></expres>
