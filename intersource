def GetLSProData(page_data,Cookie_Jar,m):
 import re
 strurl = re.findall('channels="(.*)"', page_data)[0].replace('\/', '/').replace('&quot;','"').replace('false', '[COLOR red]Offline[/COLOR]').replace('true', '[COLOR green]Online[/COLOR]')
 return strurl
]]></expres>
